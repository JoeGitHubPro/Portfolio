                    {
                        if (i % 2 == 0)
                        {
                            <div class="col-lg-6">

                                <div class="progress">
                                    <span class="skill">@item.Key<i class="val">@item.Value%</i></span>
                                    <div class="progress-bar-wrap">
                                        <div class="progress-bar" role="progressbar" aria-valuenow="@item.Value" aria-valuemin="0" aria-valuemax="@item.Value"></div>
                                    </div>
                                </div>
                            </div>
                        }

                        if (i % 2 != 0)
                        {
                            <div class="col-lg-6">

                                <div class="progress">
                                    <span class="skill">@item.Key <i class="val">@item.Value%</i></span>
                                    <div class="progress-bar-wrap">
                                        <div class="progress-bar" role="progressbar" aria-valuenow="@item.Value" aria-valuemin="0" aria-valuemax="@item.Value"></div>
                                    </div>
                                </div>
                            </div>
                        }
                        i++;
                    }
                }
